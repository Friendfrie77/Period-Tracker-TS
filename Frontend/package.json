{
  "name": "period-tracker-ts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --mode development",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "checkUpdates": "ncu",
    "upgradeDeps": "ncu -u && npm install",
    "migrate-sass": "sass-migrator module --migrate-deps styles.scss",
    "test-migrate-sass": "sass-migrator module --migrate-deps --dry-run --verbose styles.scss"
  },
  "dependencies": {
    "@e965/xlsx": "^0.20.3",
    "@meinefinsternis/react-horizontal-date-picker": "^1.0.1",
    "@reduxjs/toolkit": "^2.8.2",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.13",
    "gsap": "^3.13.0",
    "react": "^19.1.0",
    "react-countdown-circle-timer": "^3.2.1",
    "react-date-range": "^2.0.1",
    "react-dom": "^19.1.0",
    "react-final-form": "^7.0.0",
    "react-icons": "^5.5.0",
    "react-multi-carousel": "^2.8.6",
    "react-phone-input-2": "^2.15.1",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.6.3",
    "redux-persist": "^6.0.0",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.27.1",
    "@jest/types": "^30.0.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.8",
    "@types/react-big-calendar": "^1.16.2",
    "@types/react-date-range": "^1.4.10",
    "@types/react-dom": "^19.1.6",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "@vitejs/plugin-react-swc": "^3.10.2",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "jest-environment-jsdom": "^30.0.4",
    "jsdom": "^26.1.0",
    "npm-check-updates": "^18.0.1",
    "react-big-calendar": "^1.19.4",
    "sass": "^1.91.0",
    "sass-migrator": "^2.4.2",
    "typescript": "^5.8.3",
    "vite": "^7.0.1",
    "vitest": "^3.2.4"
  }
